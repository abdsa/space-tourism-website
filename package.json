{
  "name": "space-tourism-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:next dev:css",
    "build": "npm-run-all --parallel build:next build:css",
    "dev:next": "next dev",
    "build:next": "next build",
    "start": "next start",
    "dev:css": "postcss styles/globals.css --config postcss.config.js -o styles/bundle.css -w",
    "build:css": "postcss styles/globals.css --config postcss.config.js -o styles/bundle.css",
    "lint": "next lint"
  },
  "dependencies": {
    "@next/font": "^13.0.0",
    "next": "^13.0.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.12",
    "eslint": "8.26.0",
    "eslint-config-next": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-cli": "^9.1.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.1",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "webpack": "^5.0.0",
    "prettier": "^2.2.0"
  }
}
